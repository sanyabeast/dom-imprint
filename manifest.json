{
  "manifest_version": 2,
  "name": "Parcel",
  "version": "1.0",
  
  "icons": {
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "imprint.js" ]
    }
  ],
  "permissions": [
    "http://*",
    "https://*",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Parcel",
    "default_icon": "images/icon128.png"
  }
}